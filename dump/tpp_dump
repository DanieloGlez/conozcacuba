    LANGUAGE plpgsql
    AS $$
BEGIN
    IF (NEW.dist_to_airport <= 0)  THEN
        RAISE EXCEPTION 'distance to airport cant not be a negative number';
    END IF;
    RETURN NEW;
END
$$;


