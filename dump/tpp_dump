--
-- TOC entry 201 (class 1259 OID 23954)
-- Name: company_transport_id_company_transport_seq; Type: SEQUENCE; Schema: tpp; Owner: -
--

CREATE SEQUENCE tpp.company_transport_id_company_transport_seq
    AS integer
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
