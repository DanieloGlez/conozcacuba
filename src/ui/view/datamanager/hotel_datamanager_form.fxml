<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controller.modalsController.HotelModal">
   <children>
      <GridPane hgap="10.0" layoutX="100.0" layoutY="108.0" vgap="5.0">
        <columnConstraints>
